<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rdsaleh.sipu_polindra">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_sipu"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity
            android:name=".IndexActivity"
            android:theme="@style/AppThemeIndex">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppThemeLogin" />
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/AppThemeRegister" />
        <activity
            android:name=".DashboardActivity"
            android:theme="@style/AppThemeDashboard" />
        <activity android:name=".KompaActivity" />
        <activity android:name=".KopenActivity" />
        <activity android:name=".RpiActivity" />
        <activity android:name=".PopiActivity" />
        <activity android:name=".FolafoActivity" />
        <activity
            android:name=".DaftarUkmActivity"
            android:theme="@style/AppThemeDaftarUkm" />
        <activity
            android:name=".ProfileActivity"
            android:theme="@style/AppThemeProfile" />
        <activity android:name=".EditInfoActivity"
            android:theme="@style/AppThemeProfile" />

        <activity
            android:name=".IntroSlide"
            android:theme="@style/Theme.Intro" />

    </application>

</manifest>